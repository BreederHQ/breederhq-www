    {
      "source": "/dog-breeding",
      "destination": "/dogs",
      "permanent": true
    },
    {
      "source": "/dog-breeder-software",
      "destination": "/dogs",
      "permanent": true
    },
    {
      "source": "/cat-breeding",
      "destination": "/cats",
      "permanent": true
    },
    {
      "source": "/equine-breeding",
      "destination": "/horses",
      "permanent": true
    },
    {
      "source": "/horse-breeding",
      "destination": "/horses",
      "permanent": true
    },
    {
      "source": "/pricing",
      "destination": "/coming-soon",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
