{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.breederhq.com"
        }
      ],
      "destination": "https://breederhq.com/:path*",
      "permanent": true
    },
    {
      "source": "/dog-breeding",
      "destination": "/dogs",
      "permanent": true
    },
    {
      "source": "/dog-breeder-software",
      "destination": "/dogs",
      "permanent": true
    },
    {
      "source": "/cat-breeding",
      "destination": "/cats",
      "permanent": true
    },
    {
      "source": "/equine-breeding",
      "destination": "/horses",
      "permanent": true
    },
    {
      "source": "/horse-breeding",
      "destination": "/horses",
      "permanent": true
    },
    {
      "source": "/pricing",
      "destination": "/coming-soon",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
